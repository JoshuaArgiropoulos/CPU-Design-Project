module rotateLeft(value, count, result);

input [31:0] value;
input [4:0] count;
output [31:0] result;

reg [31:0] temp;

//assign result = value >> count;
//always @(*) begin
//assign result = {value[count-1:0], value[31:count]};
//end

always @(*) begin
    case(count)
      5'd1: temp = {value[0], value[31:1]};
      5'd2: temp = {value[1:0], value[31:2]};
      5'd3: temp = {value[2:0], value[31:3]};
      5'd4: temp = {value[3:0], value[31:4]};
      5'd5: temp = {value[4:0], value[31:5]};
      5'd6: temp = {value[5:0], value[31:6]};
      5'd7: temp = {value[6:0], value[31:7]};
      5'd8: temp = {value[7:0], value[31:8]};
      5'd9: temp = {value[8:0], value[31:9]};
      5'd10: temp = {value[9:0], value[31:10]};
      5'd11: temp = {value[10:0], value[31:11]};
      5'd12: temp = {value[11:0], value[31:12]};
      5'd13: temp = {value[12:0], value[31:13]};
      5'd14: temp = {value[13:0], value[31:14]};
      5'd15: temp = {value[14:0], value[31:15]};
      5'd16: temp = {value[15:0], value[31:16]};
      5'd17: temp = {value[16:0], value[31:17]};
      5'd18: temp = {value[17:0], value[31:18]};
      5'd19: temp = {value[18:0], value[31:19]};
      5'd20: temp = {value[19:0], value[31:20]};
      5'd21: temp = {value[20:0], value[31:21]};
      5'd22: temp = {value[21:0], value[31:22]};
      5'd23: temp = {value[22:0], value[31:23]};
      5'd24: temp = {value[23:0], value[31:24]};
      5'd25: temp = {value[24:0], value[31:25]};
      5'd26: temp = {value[25:0], value[31:26]};
      5'd27: temp = {value[26:0], value[31:27]};
      5'd28: temp = {value[27:0], value[31:28]};
      5'd29: temp = {value[28:0], value[31:29]};
      5'd30: temp = {value[29:0], value[31:30]};
      5'd31: temp = {value[30:0], value[31]};
      5'd32: temp = value;
      default: temp = 0; // handle invalid count values
		endcase
	end
	assign result = temp;

endmodule
